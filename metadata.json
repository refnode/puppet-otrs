{
  "name": "wiredobjects-otrs",
  "version": "0.1.0",
  "author": "wiredobjects",
  "summary": "Puppet module for managing the Open Ticket Request System (OTRS)",
  "license": "Apache 2.0",
  "source": "https://github.com/wiredobjects/puppet-otrs.git",
  "project_page": "https://github.com/wiredobjects/puppet-otrs",
